<?xml version="1.0" encoding="utf-8" ?> 
<TemamaTradingConfig>
  <Bot name="RangerPro" displayName="example" autostart="true">
    <Api exchange="Cex">
      <!-- API -->
      <PublicKey></PublicKey>
      <SecretKey></SecretKey>
      <UserID>up0000000</UserID> <!-- This is needed for CEX.IO -->
    </Api>
    
    <!-- Algo -->
    <BaseCurrency>eth</BaseCurrency>
    <FundCurrency>usd</FundCurrency>
    <MinBaseToTrade>0.05</MinBaseToTrade>
    <MinFundToTrade>70</MinFundToTrade>
    <ExecuteInterval>30</ExecuteInterval>  <!-- Interval between bot iterations in seconds-->
    <MinutesToAnalyze>60</MinutesToAnalyze>
    <VolatilityRate>2.0</VolatilityRate>  <!-- Price range to activate bot -->
    <InRangePricePercent>95</InRangePricePercent> <!-- Percent of trades to be done inside range -->
    <TakeProfit>1.15</TakeProfit>
    <PastPossibleTrades>1</PastPossibleTrades>
    <!--MaxFundToOperate>200</MaxFundToOperate-->
    <StopLossEnabled>true</StopLossEnabled>
    <StopLossPercent>1.39</StopLossPercent>    
  </Bot>
</TemamaTradingConfig>
